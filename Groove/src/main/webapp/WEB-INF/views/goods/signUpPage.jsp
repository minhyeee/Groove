<%@ page contentType="text/html;charset=UTF-8" language="java" %>
<html>
<head>
    <title>Sign up</title>
    <link rel="stylesheet" href="/css/signUpPage.css"></head>


<body>

<header>
</header>


<div class="container">

    <div class="background">

        <div class="sign-up">
            <div class="title">
                <span>Sign up</span>
            </div>
            <form action="">
                <div class="information">
                    <label>
                        <div class="label-text">
                            <span>メールアドレス</span>
                            <span class="star">*</span>
                        </div>
                        <input name="id">
                    </label>
                    <label>
                        <div class="label-text">
                            <span>パスワート</span>
                            <span class="star">*</span>
                        </div>
                        <input name="pw">
                    </label>
                    <label>
                        <div class="label-text">
                            <span>パスワード確認</span>
                            <span class="star">*</span>
                        </div>
                        <input name="pw-check">
                    </label>
                    <label>
                        <div class="label-text">
                            <span>ニックネーム</span>
                        </div>
                        <input name="nick-name">
                    </label>
                    <label>
                        <div class="label-text">
                            <span>生年月日</span>
                        </div>
                        <input type="date">
                    </label>
                    <label>
                        <div class="label-text">
                            <span>性別</span>
                        </div>
                        <input name="gender">
                    </label>
                    <button class="btn">登録</button>
                </div>
            </form>
        </div>






    </div>










</div>






<footer>
</footer>



</body>
</html>
